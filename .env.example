# PostgreSQL
DATABASE_URL=postgresql://postgres:password@127.0.0.1:5433/sfa
# Set to 'false' only for Neon.tech or self-signed certs
# PGSSL_REJECT_UNAUTHORIZED=false

# Pforce Authentication (REQUIRED for API access)
# Password for OAuth2 password grant and SOAP login
PFORCE_API_PASSWORD=change_this_to_a_strong_password
# Client credentials for OAuth2 client_credentials / authorization_code flows
# PFORCE_CLIENT_ID=your_client_id
# PFORCE_CLIENT_SECRET=your_client_secret
# Service account email for client_credentials flow
# PFORCE_SERVICE_EMAIL=service@pforce.local

# OAuth2 Redirect URI Allowlist (comma-separated, REQUIRED for /authorize)
# PFORCE_ALLOWED_REDIRECTS=https://your-app.example.com/callback

# Google OAuth 2.0 (optional)
# GOOGLE_CLIENT_ID=your_google_client_id
# GOOGLE_CLIENT_SECRET=your_google_client_secret

# CORS Allowed Origins (comma-separated)
# ALLOWED_ORIGINS=http://localhost:3000

# AI Agent Report Generation (optional)
# OPENAI_API_KEY=your_openai_api_key
# GOOGLE_AI_KEY=your_google_ai_key

# Illumina BaseSpace (optional)
# BASESPACE_ACCESS_TOKEN=your_token_here
# BASESPACE_API_URL=https://api.basespace.illumina.com
